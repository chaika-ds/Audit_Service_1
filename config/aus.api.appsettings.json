{
  "REDIS": {
    "REDIS_CONNECTION_URL": "$REDIS_CONNECTION_URL",
    "REDIS_PREFIX": "$REDIS_PREFIX"
  },
  "ELASTIC_SEARCH": {
    "ELK_CONNECTION_URL": "$ELK_CONNECTION_URL",
    "INDEXES": {
      "ELK_INDEX_AUDITLOG": "$ELK_INDEX_AUDITLOG",
      "ELK_INDEX_APPLOG": "$ELK_INDEX_APPLOG"
    }
  },
  "KAFKA": {
    "CONFIGS": {
      "KAFKA_BROKER": "$KAFKA_BROKER",
      "KAFKA_CONSUMER_GROUP": "$KAFKA_CONSUMER_GROUP",
      "KAFKA_USERNAME": "$KAFKA_USERNAME",
      "KAFKA_PASSWORD": "$KAFKA_PASSWORD",
      "KAFKA_PREFIX": "$KAFKA_PREFIX"
    },
    "KAFKA_TOPICS": {
      "KAFKA_TOPIC_AUDITLOG": "$KAFKA_TOPIC_AUDITLOG"
    },
    "HEALTH_CHECK": {
      "CRITICAL_ERRORS_COUNT": 20,
      "FOR_PERIOD_IN_SEC": 20
    }
  },
  "SSO": {
    "SSO_SERVICE_NAME": "$SSO_SERVICE_NAME",
    "SSO_AUTH_SERVICE_ID": "$SSO_AUTH_SERVICE_ID",
    "SSO_AUTH_ROOT_NODE_ID": "$SSO_AUTH_ROOT_NODE_ID",
    "SSO_SERVICE_URL": "$SSO_SERVICE_URL",
    "SSO_AUTH_API_KEY": "$SSO_AUTH_API_KEY"
  }
}