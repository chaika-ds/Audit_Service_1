{
  "RedisCache": {
    "ConnectionString": "redis:6379,abortConnect=false",
    "InstanceName": "DevelopRedisCache"
  },
  "ElasticSearch": {
    "Uris": [ "http://elasticsearch:9200", "http://localhost:9200" ],
    "UserName": "",
    "Password": "",
    "DefaultIndex": "testaudit",
    "Indexes": {
      "AuditLog": "testaudit",
      "ApplicationLog": "applog"
    }
  },
  "Kafka": {
    "MaxThreadsCount": 10,
    "MaxTimeoutMsec": 5000,
    "PermissionsTopic": "uat.sso.sync.permission",
    "PermissionsSystemName": "AditlogV2.viewAuditLog",
    "PermissionsServiceName": "AditLog",
    "Config": {
      "bootstrap.servers": "localhost:9092",
      "group.id": "AuditService-test",
      "enable.partition.eof": true,
      "log.connection.close": false,
      "enable.auto.commit": false,
      "queued.min.messages": 10,
      "queued.max.messages.kbytes": 256,
      "auto.offset.reset": "smallest",
      "socket.blocking.max.ms": 100,
      "fetch.error.backoff.ms": 200
    },
    "InputSettings": {
      "AuditLogTransactionDto": "uat.auditlog.transactions"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Information"
    }
  },
  "Health": {
    "CriticalErrorsCount": 20,
    "ForPeriodInSec": 20
  }
}