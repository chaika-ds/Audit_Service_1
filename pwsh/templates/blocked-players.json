{
  "index_patterns": ["aus-sso-blocked-players-log", "aus-sso-blocked-players-log*"],
  "settings": {
    "number_of_shards": 1
  },
  "mappings": {
    "properties": {
      "nodeId": {
        "type": "keyword"
      },
      "blockingDate": {
        "type": "date"
      },
      "previousBlockingDate": {
        "type": "date"
      },
      "blocksCounter": {
        "type": "integer"
      },
      "playerLogin": {
        "type": "keyword"
      },
      "playerId": {
        "type": "keyword"
      },
      "lastVisitIpAddress": {
        "type": "keyword"
      },
      "platform": {
        "type": "keyword"
      },
      "browser": {
        "type": "keyword"
      },
      "browserVersion": {
        "type": "keyword"
      },
      "language": {
        "type": "keyword"
      }
    }
  }
}